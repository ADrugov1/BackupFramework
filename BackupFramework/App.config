<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
<system.serviceModel>
        <behaviors>
                <serviceBehaviors>
                        <behavior name="">
                                <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                                <serviceDebug includeExceptionDetailInFaults="false" />
                        </behavior>
                </serviceBehaviors>
        </behaviors>
        <services>
                <service name="BackupFramework.Service1">
                        <endpoint address="" binding="basicHttpBinding" contract="BackupFramework.IService1">
                                <identity>
                                        <dns value="localhost" />
                                </identity>
                        </endpoint>
                        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                        <host>
                                <baseAddresses>
                                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/BackupFramework/Service1/" />
                                </baseAddresses>
                        </host>
                </service>
        </services>
        <bindings />
        <client />
    </system.serviceModel>
</configuration>